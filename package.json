    "@babel/polyfill": "^7.12.1",
    "@sendgrid/mail": "^8.1.6",
    "@stripe/stripe-js": "^8.0.0",
    "axios": "^1.12.2",
    "axois": "^0.0.1-security",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "core-js": "^2.6.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
