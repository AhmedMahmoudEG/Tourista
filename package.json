{
  "name": "tourista",
  "version": "1.0.0",
  "description": "Learning Express, Nodejs and MongoDB",
  "main": "app.js",
  "scripts": {
    "start": "node --watch server.js",
    "start:prod": "cross-env NODE_ENV=production node --watch server.js",
    "debug:dev": "cross-env NODE_ENV=development ndb nodemon server.js",
    "debug:prod": "cross-env NODE_ENV=production ndb nodemon server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js --no-source-maps",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js"
  },
  "keywords": [],
  "author": "Ahmad Mahmoud",
  "license": "ISC",
